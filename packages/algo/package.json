{
  "name": "@ds-algo.ts/algo",
  "version": "0.0.0",
  "description": "High-performance TypeScript algorithms library",
  "type": "module",
  "main": "./dist/algo.umd.cjs",
  "module": "./dist/algo.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/algo.js",
      "require": "./dist/algo.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./sorting": {
      "import": "./dist/sorting/index.js",
      "types": "./dist/sorting/index.d.ts"
    },
    "./searching": {
      "import": "./dist/searching/index.js",
      "types": "./dist/searching/index.d.ts"
    },
    "./graph": {
      "import": "./dist/graph/index.js",
      "types": "./dist/graph/index.d.ts"
    },
    "./string": {
      "import": "./dist/string/index.js",
      "types": "./dist/string/index.d.ts"
    },
    "./dp": {
      "import": "./dist/dynamic-programming/index.js",
      "types": "./dist/dynamic-programming/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "rm -rf dist && tsc && vite build",
    "clean": "rm -rf dist node_modules",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "algorithms",
    "sorting",
    "searching",
    "graph-algorithms",
    "dynamic-programming"
  ],
  "license": "MIT",
  "dependencies": {
    "@ds-algo.ts/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
