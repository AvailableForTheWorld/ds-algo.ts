{
  "name": "@ds-algo.ts/shared",
  "version": "0.0.0",
  "description": "Shared types and utilities for ds-algo.ts",
  "type": "module",
  "main": "./dist/shared.umd.cjs",
  "module": "./dist/shared.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/shared.js",
      "require": "./dist/shared.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "rm -rf dist && tsc && vite build",
    "clean": "rm -rf dist node_modules",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "types",
    "data-structures",
    "algorithms"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  }
}
