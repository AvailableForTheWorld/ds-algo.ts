{
  "name": "ds-algo.ts",
  "version": "0.0.0",
  "private": true,
  "description": "High-performance TypeScript data structures and algorithms library",
  "scripts": {
    "build": "pnpm -r --filter='./packages/*' run build",
    "build:shared": "pnpm --filter @ds-algo.ts/shared build",
    "build:ds": "pnpm --filter @ds-algo.ts/ds build",
    "build:algo": "pnpm --filter @ds-algo.ts/algo build",
    "dev": "pnpm -r --parallel run dev",
    "test": "pnpm -r run test",
    "test:ds": "pnpm --filter @ds-algo.ts/ds test",
    "test:algo": "pnpm --filter @ds-algo.ts/algo test",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "type-check": "pnpm -r run type-check",
    "build:libs": "vite build --mode shared && vite build --mode ds && vite build --mode algo",
    "dev:libs": "vite build --mode shared --watch"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.12.1"
}