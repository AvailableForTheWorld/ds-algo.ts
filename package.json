{
  "name": "ds-algo.ts",
  "version": "0.0.0",
  "private": true,
  "description": "High-performance TypeScript data structures and algorithms library",
  "scripts": {
    "build": "pnpm build:shared && pnpm build:ds && pnpm build:algo",
    "build:shared": "vite build --mode shared",
    "build:ds": "vite build --mode ds",
    "build:algo": "vite build --mode algo",
    "dev": "pnpm -r --parallel run dev",
    "test": "vitest run",
    "test:ds": "pnpm --filter @ds-algo.ts/ds test",
    "test:algo": "pnpm --filter @ds-algo.ts/algo test",
    "test:packages": "pnpm build && pnpm --filter examples test-packages",
    "example:stack": "pnpm --filter examples stack-example",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "type-check": "tsc --noEmit --project tsconfig.base.json",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,mjs,json,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.12.1"
}
